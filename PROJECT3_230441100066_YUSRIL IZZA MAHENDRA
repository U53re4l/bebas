celcius = int(input("masukkan suhu celcius: "))

farenheit = (9/5*celcius)+32
reamur = 4/5*celcius
kelvin = celcius+273

print("suhu dalam bentuk farenheit: ", farenheit, "derajat farenheit")
print("suhu dalam bentuk reamur: ", reamur, "derajat reamur")
print("suhu dalam bentuk kelvin: ", kelvin,  "derajat kelvin")
