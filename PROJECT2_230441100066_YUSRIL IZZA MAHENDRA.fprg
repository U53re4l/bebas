            <input variable="d"/>
            <assign variable="e" expression="d-b"/>
            <assign variable="f" expression="a*c"/>
            <assign variable="g" expression="f+(f*(0.1*e))"/>
            <output expression="&quot;harga emas anda ditahun &quot; &amp;d &amp;&quot;adalah &quot; &amp;g" newline="True"/>
