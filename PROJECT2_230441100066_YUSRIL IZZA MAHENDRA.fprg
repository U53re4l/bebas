    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c, d, e" type="Integer" array="False" size=""/>
            <declare name="f, g" type="Real" array="False" size=""/>
            <output expression="&quot;Masukan jumlah emas yang anda miliki (gram):&quot;" newline="True"/>
            <input variable="a"/>
            <output expression="&quot;Tahun saat ini: &quot;" newline="True"/>
            <input variable="b"/>
